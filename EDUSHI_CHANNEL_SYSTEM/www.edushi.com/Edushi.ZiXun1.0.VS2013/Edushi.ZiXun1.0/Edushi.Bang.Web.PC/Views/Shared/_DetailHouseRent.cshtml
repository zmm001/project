@{
    string baseUrl = "http://fangtoo.edushi.com";
    string JsonUrl = "http://hangzhou.fangtoo.com/cz/gethouse/";
    long ticks = DateTime.Now.Ticks;    //时间戳
}

<script type="text/javascript">
    (function ($) {
        $(document).ready(function () {
            $.getJSON("@JsonUrl?timeTicks=@ticks&ps=1&callback=?", function (data) {
                if (!data || !data.data) return;
                var _item = data.data[0], _html = "";
                _html += '<a href="@baseUrl/cz/' + _item.id + '/" target="_blank" title="' + _item.BuildingName + '"><img alt="' + _item.BuildingName + '" src="http://imgs.fangtuwang.com/' + _item.Pics.replace("s_", "m_") + '" style="width:300px; height:179px;" /></a>';
                _html += '<div style="position: absolute; width:100%; height:50px; bottom: 0px; left: 0px;background: #ffffff; opacity: 0.7; filter: alpha(opacity=70); font-size: 12px;">';
                _html += '<div style="text-indent: 10px; line-height: 28px; font-size: 14px; "><a href="@baseUrl/cz/' + _item.id + '/" target="_blank" title="' + _item.BuildingName + '" style="color: #000000; font-weight:bold; ">' + _item.BuildingName + '</a></div>';
                _html += '<div style="text-indent: 10px;"><span style="margin-right:0.5em;">' + _item.RegionName + '</span><span style="margin-right:0.5em;">' + (_item.RoomCount > 0 ? _item.RoomCount + "室" : "") + (_item.HollCount > 0 ? _item.HollCount + "厅" : "") + '</span><span style="margin-right:0.5em;">' + _item.Floor + '/' + _item.FloorCount + '层</span>'+_item.DeckName+'</div>';
                _html += '</div>';
                _html += '<div style=" font-size: 14px; color: #ff0000; position: absolute; right: 15px; bottom: 13px;"><b style="font-weight: bold; font-size: 20px; ">' + _item.RefPrice + '</b>' + (_item.PriceUnit&& _item.PriceUnit == 352 ? "元/m²·天" : "元/月") + '</div>';
                $("#ft_right_cz").html(_html);
            });
        });
    })(jQuery);
</script>


<div id="ft_right_cz" style=" position: relative; width: 300px; height: 179px; margin-bottom:20px;">
</div>
					<readerQuotas maxDepth="32" maxStringContentLength="6553600" maxArrayLength="16384" maxBytesPerRead="4096" maxNameTableCharCount="6553600"/>
					<security mode="None">
						<transport clientCredentialType="None" proxyCredentialType="None" realm=""/>

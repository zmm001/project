		<client>
			<endpoint address="http://webag.edushi.com/jd/AlaHotelService.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAalService" contract="IAalService" name="BasicHttpBinding_IAalService"/>
